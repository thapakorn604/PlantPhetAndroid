<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ploychanok.subscriber">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".homeActivity" android:screenOrientation="locked"></activity>
        <activity android:name=".profileActivity" android:screenOrientation="locked"></activity>
        <activity android:name=".SignupActivity" android:screenOrientation="locked"></activity>
        <activity android:name=".FAQ.faqActivity" android:screenOrientation="locked"></activity>
        <activity android:name=".FAQ.aboutActivity" android:screenOrientation="locked"></activity>
        <activity android:name=".FAQ.faqInvestorActivity" android:screenOrientation="locked"></activity>
        <activity android:name=".FAQ.contactActivity" android:screenOrientation="locked"></activity>
        <activity android:name=".FAQ.manualActivity" android:screenOrientation="locked"></activity>
        <activity android:name=".Farmerlist.farmerlistActivity" android:screenOrientation="locked"></activity>
    </application>

</manifest>